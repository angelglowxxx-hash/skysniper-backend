{
  "name": "skysniper-backend",
  "version": "1.9.3",
  "description": "SkySniper backend API for hash decoding, AI prediction (Gemini 2.5 Flash), fingerprint memory, Supabase sync, HUD rendering, site classification, and admin control",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "prestart": "npm run lint && npm test",
    "start": "node server.js",
    "dev": "nodemon --delay 2 --watch . server.js",
    "lint": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "health": "node health-check.js"
  },
  "keywords": [
    "aviator",
    "crash-game",
    "hash-decoder",
    "ai-prediction",
    "supabase",
    "gemini",
    "skysniper",
    "fingerprint",
    "self-evolving",
    "browser-toolkit",
    "hud",
    "automation",
    "admin-panel",
    "config-pusher",
    "site-classifier",
    "tampermonkey",
    "stealth-mode",
    "trend-analysis",
    "gemini-flash",
    "live-dashboard",
    "modular-api",
    "render-deploy"
  ],
  "author": "Honey Baby ğŸ’¥",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.14.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "express-rate-limit": "^7.0.0",
    "pino-http": "^8.0.0",
    "@sentry/node": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "nodemon": "^3.0.2",
    "jest": "^29.6.1",
    "supertest": "^7.1.3",
    "prettier": "^3.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.0"
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "npm@9.0.0"
}
